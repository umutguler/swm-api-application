            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "watch_dev",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/Swm.Api.Business/bin/Debug/net6.0/Swm.Api.Business.dll",
